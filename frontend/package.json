{
  "name": "sell-your-records",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@fontsource/roboto": "^4.3.0",
    "@material-ui/core": "^5.0.0-alpha.33",
    "@material-ui/icons": "^5.0.0-alpha.29",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-material-ui-carousel": "^2.2.6",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/**/*.{js,jsx}",
    "lint:fix": "eslint ./src/**/*.{js,jsx} --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "test:all": "run-script-os",
    "test:all:windows": "set CI=true && react-scripts test && npm run lint",
    "test:all:default": "CI=true react-scripts-ts test && npm run lint",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^5.1.2",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.4",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.8",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "pretty-quick": "^3.1.0",
    "run-script-os": "^1.1.6"
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --fix",
    "*.{json,css,md,js,jsx}": "prettier --write "
  },
  "husky": {
    "hooks": {
      "pre-commit": "sh ../hooks/pre-commit && lint-staged",
      "pre-push": "npm run lint"
    }
  }
}
